<h1>
  Like list
</h1>

<a href="/new_like_form">
  Add a new like
</a>

<hr>

<table>
  <tr>
    <th>
      ID
    </th>

    <th>
      Story
    </th>

    <th>
      Fan
    </th>

    <th>
      Created at
    </th>

    <th>
      Updated at
    </th>

    <th>
    </th>
  </tr>

  <% @likes.each do |like| %>
  <tr>
    <td>
      <%= like.id %>
    </td>

    <td>
      <%= like.story_id %>
    </td>

    <td>
      <%= like.fan_id %>
    </td>

    <td>
      <%= time_ago_in_words(like.created_at) %> ago
    </td>

    <td>
      <%= time_ago_in_words(like.updated_at) %> ago
    </td>

    <td>
      <a href="/likes/<%= like.id %>">
        Show details
      </a>
    </td>
  </tr>
  <% end %>
</table>
